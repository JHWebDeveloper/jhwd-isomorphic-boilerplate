{
	"presets": [
		"@babel/preset-react",
		[
			"@babel/preset-env",
			{
				"useBuiltIns": "usage",
				"corejs": {
					"version": 3
				}
			}
		],
		[
			"minify",
			{
				"builtIns": false
			}
		]
	],
	"plugins": [
		"@babel/plugin-proposal-nullish-coalescing-operator",
		"@babel/plugin-proposal-optional-chaining"
	]
}
